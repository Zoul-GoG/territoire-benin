                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/departements/{dept}/communes": {
            "get": {
                "tags": [
                    "Territoire"
                ],
                "summary": "Récupérer les départements",
                "description": "Retourne la liste de tous les départements",
                "parameters": [
                    {
                        "name": "dept",
                        "in": "path",
                        "description": "Nom du département dont on veut connaître les communes",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "DONGA"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Succès",
                        "content": {
                            "application/json": {
                                "example": {
                                    "id_dep": 7,
                                    "lib_dep": "DONGA",
                                    "communes": [
                                        {
                                            "id_com": 44,
                                            "lib_com": "BASSILA",
                                            "arrondissements": [
                                                {
                                                    "id_arrond": 316,
                                                    "lib_arrond": "ALEDJO",
                                                    "quartiers": [
                                                        {
                                                            "id_quart": 3080,
                                                            "lib_quart": "AKARADE"
                                                        }
                                                    ]
                                                }
                                            ]
                                
                                        }
                                    ]
                                },
                                "schema": {
                                    "type" : "object",
                                    "properties": {
                                        "type": "object",
                                        "items": {
                                            "id_dep": {
                                                "type": "integer",
                                                "example": 7,
                                                "description": "id du département dans la bdd"
                                            },
                                            "lib_dep": {
                                                "type": "string",
                                                "example": "DONGA",
                                                "description": "Nom du département"
                                            },
                                            "communes": {
                                                "type": "array",
                                                "description": "Liste des communes du département",
                                                "properties": {
                                                    "type": "object",
                                                    "items": {
                                                        "id_com": {
                                                            "type": "integer",
                                                            "example": 44
                                                        },
                                                        "lib_com": {
                                                            "type": "string",
                                                            "example": "DJOUGOU",
                                                            "description": "Nom d'une commune du département"
                                                        },
                                                        "arrondissements": {
                                                            "type": "array",
                                                            "description": "Liste des arrondissements de la commune du département",
                                                            "properties": {
                                                                "type": "object",
                                                                "items": {
                                                                    "id_arrond": {
                                                                        "type": "integer",
                                                                        "example": 316
                                                                    },
                                                                    "lib_arrond": {
                                                                        "type": "string",
                                                                        "example": "SEROU",
                                                                        "description": "Nom d'un arrondissement d'une commune du département"
                                                                    },
                                                                    "quartiers": {
                                                                        "type": "array",
                                                                        "description": "Liste quartiers de l'arrondissement de la commune du département choisi",
                                                                        "properties": {
                                                                            "type": "object",
                                                                            "items": {
                                                                                "id_quart": {
                                                                                    "type": "integer",
                                                                                    "example": 3160
                                                                                },
                                                                                "lib_quart": {
                                                                                    "type": "string",
                                                                                    "example": "ZOUNTORI",
                                                                                    "description": "Nom d'un quartier de l'arrondissement d'une commune du département choisi"
                                                                                }
                                                                            }
                                                                        }
            
                                                                    }
                                                                }
                                                            }
            
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
