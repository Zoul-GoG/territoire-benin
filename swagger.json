                                    "quartiers 1",
                                    "quartiers 2",
                                    "quartiers 3"
                                ],
                                "schema": {
                                    "type" : "array",
                                    "properties": {
                                        "items": {
                                            "type": "string",
                                            "example": [
                                                "quartiers 1",
