{
    "openapi": "3.0.0",
    "info": {
      "version": "1.0.0",
      "title": "territoire-bj",
      "description": "Cette API vous fournit les informations sur les départements, communes, arrondissements et quartiers/villages du Bénin",
      "license": {
        "name": "MIT",
        "url": "https://opensource.org/licenses/MIT"
      }
    },
    "servers": [
      {
        "url": "https://territoire-bj.onrender.com",
        "description": "Prod Development Server"
      }
    ],
    "tags": [
      {
        "name": "Territoire",
        "description": "Routes de récupération des départements/communes/arrondissements/quartiers du Bénin"
      }
    ],
    "paths": {
        "/v1/departements": {
            "get": {
                "tags": [
                    "Territoire"
                ],
                "summary": "Récupérer les départements",
                "description": "Retourne la liste de tous les départements",
                "responses": {
                    "200": {
                        "description": "Succès",
                        "content": {
                            "application/json": {
                                "example": 
                                [
                                    "ATACORA",
                                    "DONGA"
                                ],
                                "schema": {
                                    "type" : "array",
                                    "properties": {
                                        "items": {
                                            "type": "string",
                                            "example": [
                                                "ATACORA",
                                                "DONGA"            
                                            ],
                                            "description": "Nom de département du Bénin"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/communes": {
            "get": {
                "tags": [
                    "Territoire"
                ],
                "summary": "Récupérer les communes",
                "description": "Retourne la liste de toutes les communes",
                "responses": {
                    "200": {
                        "description": "Succès",
                        "content": {
                            "application/json": {
                                "example": 
                                [
                                    "ABOMEY-CALAVI",
                                    "BASSILA",
                                    "COTONOU",
                                    "DJOUGOU",
                                    "PARAKOU"
                                ],
                                "schema": {
                                    "type" : "array",
                                    "properties": {
                                        "items": {
                                            "type": "string",
                                            "example": [
                                                "COTONOU",
                                                "DJOUGOU"
                                            ],
                                            "description": "Nom de commune du Bénin"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/arrondissements": {
            "get": {
                "tags": [
                    "Territoire"
                ],
                "summary": "Récupérer les arrondissements",
                "description": "Retourne la liste de tous les arrondissements",
                "responses": {
                    "200": {
                        "description": "Succès",
                        "content": {
                            "application/json": {
                                "example": 
                                [
                                    "arrondissement 1",
                                    "arrondissement 2",
                                    "arrondissement 3"
                                ],
                                "schema": {
                                    "type" : "array",
                                    "properties": {
                                        "items": {
                                            "type": "string",
                                            "example": [
                                                "arrondissement 1",
                                                "arrondissement 2",
                                                "arrondissement 3"
                                            ],
                                            "description": "Nom d'arrondissement du Bénin"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/quartiers": {
            "get": {
                "tags": [
                    "Territoire"
                ],
                "summary": "Récupérer les quartiers",
                "description": "Retourne la liste de tous les quartiers",
                "responses": {
                    "200": {
                        "description": "Succès",
                        "content": {
                            "application/json": {
                                "example": 
